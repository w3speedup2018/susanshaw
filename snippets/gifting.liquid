{% assign icon_first = shop.metafields.sf_product_data.icon_first %}
{% assign content_first = shop.metafields.sf_product_data.content_first %}
{% assign icon_second = shop.metafields.sf_product_data.icon_second %}
{% assign content_second = shop.metafields.sf_product_data.content_second %}
{% assign icon_third = shop.metafields.sf_product_data.icon_third %}
{% assign content_third = shop.metafields.sf_product_data.content_third %}
{% assign icon_fourth = shop.metafields.sf_product_data.icon_fourth %}
{% assign content_fourth = shop.metafields.sf_product_data.content_fourth %}
{% assign icon_five = shop.metafields.sf_product_data.icon_five %}
{% assign content_five = shop.metafields.sf_product_data.content_five %}
{% assign hover_image_first = shop.metafields.sf_product_data.hover_image_first %}
{% assign hover_image_second = shop.metafields.sf_product_data.hover_image_second %}
{% assign hover_image_third = shop.metafields.sf_product_data.hover_image_third %}
{% assign hover_image_fourth = shop.metafields.sf_product_data.hover_image_fourth %}
{% assign hover_image_fifth = shop.metafields.sf_product_data.hover_image_fifth %}

<div class ="five_icons custom_deskpro" >
  <div class="five_icons_inner">
    <div class="fi_sub">
      <img class="main_image lazyimage" src="{{ icon_first | replace: ".png", "_x300.png" | replace: ".jpg", "_x300.jpg" | replace: ".jpeg", "_x300.jpeg" }}" 
           data-src="{{ icon_first | replace: ".png", "_x300.png" | replace: ".jpg", "_x300.jpg" | replace: ".jpeg", "_x300.jpeg" }}" >
      <img class="hover_image lazyimage" src="{{ hover_image_first }}" 
           data-src="{{ hover_image_first }}" >
      {{ content_first }}
    </div>
    <div class="fi_sub">
      <img class="main_image lazyimage" src="{{ icon_second | replace: ".png", "_x300.png" | replace: ".jpg", "_x300.jpg" | replace: ".jpeg", "_x300.jpeg" }}" 
           data-src="{{ icon_second | replace: ".png", "_x300.png" | replace: ".jpg", "_x300.jpg" | replace: ".jpeg", "_x300.jpeg" }}" >
      <img class="hover_image lazyimage" src="{{ hover_image_second }}" 
           data-src="{{ hover_image_second }}" >
      {{ content_second }}  
    </div>
    <div class="fi_sub">
      <img class="main_image lazyimage" src="{{ icon_third | replace: ".png", "_x300.png" | replace: ".jpg", "_x300.jpg" | replace: ".jpeg", "_x300.jpeg" }}" 
           data-src="{{ icon_third | replace: ".png", "_x300.png" | replace: ".jpg", "_x300.jpg" | replace: ".jpeg", "_x300.jpeg" }}" >
      <img class="hover_image lazyimage" src="{{ hover_image_third }}" 
           data-src="{{ hover_image_third }}" >
      {{ content_third }}  
    </div>
    <div class="fi_sub">
      <img class="main_image lazyimage" src="{{ icon_fourth | replace: ".png", "_x300.png" | replace: ".jpg", "_x300.jpg" | replace: ".jpeg", "_x300.jpeg" }}" 
           data-src="{{ icon_fourth | replace: ".png", "_x300.png" | replace: ".jpg", "_x300.jpg" | replace: ".jpeg", "_x300.jpeg" }}" >
      <img class="hover_image lazyimage" src="{{ hover_image_fourth }}" 
           data-src="{{ hover_image_fourth }}" >
      {{ content_fourth }}  
    </div>
    <div class="fi_sub">
      <img class="main_image lazyimage" src="{{ icon_five | replace: ".png", "_x300.png" | replace: ".jpg", "_x300.jpg" | replace: ".jpeg", "_x300.jpeg" }}" 
           data-src="{{ icon_five | replace: ".png", "_x300.png" | replace: ".jpg", "_x300.jpg" | replace: ".jpeg", "_x300.jpeg" }}">
      <img class="hover_image lazyimage" src="{{ hover_image_fifth }}" 
           data-src="{{ hover_image_fifth }}" >
      {{ content_five }} 
    </div>
  </div>
</div>

<style>
  @media only screen and (max-width: 900px) and (min-width: 320px) {
    .five_icons.custom_deskpro {
      display: block !important;
      border: navajowhite;
      margin-top: 0px;
      padding-top: 0px;
    }

    .five_icons.custom_deskpro .five_icons_inner {
      display: flex;
    }

    .five_icons.custom_deskpro .five_icons_inner .fi_sub {
      width: 100% !important;
      flex-shrink: 0;
    }

    .five_icons.custom_deskpro .five_icons_inner.flickity-enabled {
      display: block !important;
      width: 100% !important;
    }


    .five_icons_inner.flickity-enabled.is-draggable .fi_sub {
      width: 100% !important;
      /*           height: 100% !important; */
    }

    .five_icons_inner.flickity-enabled.is-draggable .fi_sub img {
      /*           width: 100% !important;
      height: auto !important;
      object-fit: contain; */
    }

    .five_icons_inner.flickity-enabled button.flickity-prev-next-button.next svg .arrow,
    .five_icons_inner.flickity-enabled button.flickity-prev-next-button.previous svg .arrow {
      fill: #000 !important;
      opacity: 1;
    }
    .five_icons_inner.flickity-enabled .flickity-prev-next-button.previous,
    .five_icons_inner.flickity-enabled .flickity-prev-next-button.next {
      background: transparent;
    }

    .five_icons_inner.flickity-enabled button.flickity-prev-next-button.previous svg,
    .five_icons_inner.flickity-enabled button.flickity-prev-next-button.next svg {
      height: 30px;
      width: 30px;
      border-radius: 100px;
      background: #f2f2f2 !important;
      opacity: 1;
      padding: 7px 0;
    }
  }
</style>

<script type="text/javascript">
  window.addEventListener("load", () => {
    requestIdleCallback(() => {
      let slide_match = window.matchMedia('screen and (min-width: 900px)');
        function productInfoSlider(e) {
          if (e.matches) {
            $('.custom_deskpro .five_icons_inner.flickity-enabled').flickity('destroy');
          } else {
            requestIdleCallback(() => {
               $('.custom_deskpro .five_icons_inner').flickity({
                  autoPlay: 2000,
                  wrapAround: true,
                  pageDots: false
            	});
            });              
          }
      	}
      slide_match.addListener(productInfoSlider);
      productInfoSlider(slide_match)
    });  
  });
</script>


