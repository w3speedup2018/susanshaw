									<svg version="1.1" width="16" height="16" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"viewBox="0 0 100 100" style="enable-background:new 0 0 100 100;" xml:space="preserve"><g><path fill="currentColor" d="M90.6,2.5h-8c-1.9,0-3.5,1.4-3.7,3.4L77,24.9H11.4c-1.5,0-2.8,0.7-3.8,1.8c-0.9,1.2-1.2,2.7-0.8,4.1l10.7,41.9c0.5,2.1,2.4,3.6,4.6,3.6H73c0.3,0,0.7,0,1-0.1c0.2,0,0.3,0.1,0.5,0.1c0.1,0,0.2,0,0.3,0c1.4,0,2.6-1.1,2.7-2.5l0.2-1.7c0,0,0,0,0,0l4.9-47.1h-0.1L84.2,8h6.4c1.5,0,2.8-1.2,2.8-2.8S92.2,2.5,90.6,2.5z M12.3,30.4h64.1l-4.1,40.4H22.7L12.3,30.4z"/><circle fill="currentColor" cx="31.2" cy="89.4" r="8"/><circle fill="currentColor" cx="64.2" cy="89.4" r="8"/></g></svg>
								{%- endcapture -%}
								{% if product.available %}
									{% if product.variants.size > 1 %}
										<a href="{{ product.url | within: collection }}" class="lh-btn-product-options lh-tooltip_d" data-handle="{{product.handle}}" data-pid="{{product.id}}" data-lh-tooltip="{{'xnw4ifm1ft.lh_txhi7h_shop' | t }}">
											{{icon_cart}}
										</a>
									{% else %}
										<a href="/cart/add/{{product.variants.first.id}}" data-id="{{product.variants.first.id}}"  class="lh-btn-add-to-cart lh-tooltip_d" data-handle="{{product.handle}}" data-pid="{{product.id}}" data-lh-tooltip="{{'xnw4ifm1ft.lh_8e01_cart' | t }}">
											{{icon_cart}}
										</a>
									{% endif %}
								{% else %}
									<a href="{{ product.url | within: collection }}" class="lh-btn-outstock lh-tooltip_d" data-handle="{{product.handle}}" data-pid="{{product.id}}" data-lh-tooltip="{{'xnw4ifm1ft.lh_hth6t_more' | t }}">
										{{icon_cart}}
									</a>
								{% endif %}
							</div>
						</div>

						<div class="lh-product-badges">
									{% if product.available == false %}
									<span class="lh-sold-out"  data-lhi="soldout_text">Sold out</span>
									{% else %}
										{% if  product.selected_or_first_available_variant.compare_at_price > product.selected_or_first_available_variant.price %}
											{% assign onehundred = 100 %}
											<span class="lh-onsale"  data-lhi="text_sale">Sale</span>

										{% elsif product.compare_at_price_varies %}
											{% assign onehundred = 100 %}
											<span class="lh-onsale"  data-lhi="text_sale">Sale</span>
										{% endif%}
										{% if product.tags contains 'new' %}
											<span class="lh-hot"  data-lhi="trans_new_text">New</span>
										{% endif %}
									{% endif %}
						</div>
					</div>
					
					<div class="lh-product-info ">
						<h3 class="lh-product-title">
							<a href="{{product.url | within: collection}}">{{product.title}}</a>
						</h3>
						<div class="lh-wrap-price">
							{% if product.compare_at_price_max > product.price %}
								<span class="lh-price-set lh-sale">{{product.compare_at_price_max | money}}</span>
							{% endif %}
							<span class="lh-price-set lh-cost">{{product.price | money}}</span>
						</div>
											
					</div>
				</div>
			</div>
			{% endfor %}</div>
	</div>
</div>

